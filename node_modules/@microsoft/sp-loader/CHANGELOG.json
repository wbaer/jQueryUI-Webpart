{
  "name": "@microsoft/sp-loader",
  "entries": [
    {
      "version": "1.0.0",
      "tag": "@microsoft/sp-loader_v1.0.0",
      "date": "Fri, 17 Feb 2017 23:09:23 GMT",
      "comments": {
        "patch": [
          {
            "comment": "Fixed the way react-dom and react are referenced in tests against the NPM module."
          },
          {
            "comment": "Ensured two copies of core libraries don't get loaded on the workbench."
          },
          {
            "comment": "loadComponent() has a retry strategy when it fails to load"
          },
          {
            "comment": "Upgraded React to 15.4.1"
          },
          {
            "comment": "Bumped up office-ui-fabric-react version to 0.83.0"
          },
          {
            "comment": "Added alias to manifest dev console"
          },
          {
            "comment": "Support global dependencies in path loader configurations"
          },
          {
            "comment": "Old versions of Firefox (<44) are not supported"
          },
          {
            "comment": "Calling loadScript with a string as the second parameter should throw an error"
          },
          {
            "comment": "Requests for React 0.14.8 return React 15.4.2"
          },
          {
            "comment": "Locked version numbers for @types packages"
          },
          {
            "comment": "Updated .npmignore"
          },
          {
            "comment": "Added query parameter (enableConsoleLog) to enable TraceLogger logging to console\""
          },
          {
            "comment": "Added tryGetManifest() function to ManifestStore and tryGetComponentById() function to ComponentStore."
          },
          {
            "comment": "IE 9 is not supported anymore."
          },
          {
            "comment": "Chrome developer tools now shows source code correctly"
          }
        ],
        "minor": [
          {
            "comment": "Support for multiple component versions simultaneously"
          },
          {
            "comment": "Updated to TypeScript 2.1"
          },
          {
            "comment": "Enabled strictNullChecks in sp-loader project."
          },
          {
            "comment": "Switched back to SystemJS 0.19.25"
          }
        ],
        "major": [
          {
            "comment": "General availability"
          }
        ]
      }
    },
