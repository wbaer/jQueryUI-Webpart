          {
            "comment": "API clean up. 'forceLoadDebugManifests' and 'loadManifestModules' are deprecated."
          },
          {
            "comment": "Localizing the strings in the module loader."
          },
          {
            "comment": "Updating the developer tools to use office-ui-fabric-react."
          }
        ],
        "patch": [
          {
            "comment": "Cleaning up dependencies."
          }
        ]
      }
    },
    {
      "version": "0.4.0",
      "tag": "@microsoft/sp-loader_v0.4.0",
      "comments": {
        "minor": [
          {
            "comment": "Adds a field to WebPart manifest for disabling the WebPart on a Classic page"
          },
          {
            "comment": "IClientSideWebPartManifestInstance doesn't extend IClientSideWebPartManifestEntry anymore"
          },
          {
            "comment": "For manifests, the `id` field should be a GUID"
          }
        ],
        "patch": [
          {
            "comment": "Updates standard library modules list to include `@microsoft/sp-webpart-base`"
          }
        ]
      }
